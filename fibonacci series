n = int(input("enter the no. upto which youwant to print fibonacci series "))

n1, n2 = 0, 1
i = 0

if n <= 0:
   print("Please enter a positive integer")
elif n == 1:
   print("Fibonacci sequence upto",nterms,":")
   print(n1)
else:
   print("Fibonacci sequence:")
   while i <n:
       print(n1)
       nth = n1 + n2
       
       n1 = n2
       n2 = nth
       i += 1
